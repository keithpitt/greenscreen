<% @projects.each do |project| %>
  <div class="project status <%= project.last_build_status %> <%= project.activity %>" data-name="<%= project.name %>" data-status="<%= project.last_build_status %>">
    <div class="border">
      <h1><a href="<%= project.web_url%>"><%= project.name %></a></h1>
      <p>Build <strong><%= project.last_build_label %></strong> was a <strong><%= project.last_build_status %></strong> <span class="build_time" title="<%= project.last_build_time %>"><%= project.last_build_time %></span></p>
    </div>
  </div>
<% end %>
